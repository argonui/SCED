# Header: Options
msgid "OP_Header"
msgstr "Opções"

# Group: general settings
msgid "OP_group_general"
msgstr "CONFIGURAÇÕES GERAIS"

# Option: UI language
msgid "OP_uiLanguage"
msgstr "Idioma da UI"

# Tooltip: UI language
msgid "OP_uiLanguage_tooltip"
msgstr "Define o idioma para os elementos da UI\ncomo este painel de opções."

# Option: splash scenario name on setup
msgid "OP_showTitleSplash"
msgstr "Mostrar o título do cenário na configuração"

# Tooltip: splash scenario name on setup
msgid "OP_showTitleSplash_tooltip"
msgstr "Aparece o nome do cenário por 2 segundos\nao colocar um cenário."

# Option: splash token on draw
msgid "OP_showTokenSplash"
msgstr "Mostrar token ampliado na tela"

# Tooltip: splash token on draw
msgid "OP_showTokenSplash_tooltip"
msgstr "Mostra uma versão grande do token sorteado\npor 2 segundos ao ser sorteado."

# Option: Enable all card helpers
msgid "OP_enableCardHelpers"
msgstr "Ativar todos os ajudantes de cartas"

# Tooltip: Enable all card helpers
msgid "OP_enableCardHelpers_tooltip"
msgstr "Ativa todos os ajudantes de cartas (geralmente ativados por entradas do menu de contexto).\nExemplos: Falso Acordo e Livro dos Mitos Vivos"

# Option: Make damage / horror tokens clickable
msgid "OP_makeDamageHorrorClickable"
msgstr "Tornar tokens de dano / terror clicáveis"

# Tooltip: Make damage / horror tokens clickable
msgid "OP_makeDamageHorrorClickable_tooltip"
msgstr "Torna os tokens de dano e terror clicáveis\n(além de suportar a digitação de números)."

# Option: Show '1' on damage / horror tokens
msgid "OP_showOneDamageHorror"
msgstr "Mostrar um 1 nos tokens de dano / terror"

# Tooltip: Show '1' on damage / horror tokens
msgid "OP_showOneDamageHorror_tooltip"
msgstr "Em vez de tokens em branco (como eles são fisicamente),\nisso adicionará um '1' ao estado padrão."

# Group: play area settings
msgid "OP_group_playarea"
msgstr "CONFIGURAÇÕES DA ÁREA DE JOGO"

# Option: play area snap tags
msgid "OP_playAreaSnapTags"
msgstr "Ativar tags de encaixe"

# Tooltip: play area snap tags
msgid "OP_playAreaSnapTags_tooltip"
msgstr "Apenas cartas com a tag 'Localização' se encaixarão (cartas oficiais são suportadas por padrão).\nDesative isso se estiver tendo problemas com conteúdo personalizado."

# Option: location connections
msgid "OP_playAreaConnections"
msgstr "Desenhar conexões de localização"

# Tooltip: location connections
msgid "OP_playAreaConnections_tooltip"
msgstr "Desenha automaticamente conexões de localização\ncom base nos metadados da carta."

# Option: play area connection color
msgid "OP_playAreaConnectionColor"
msgstr "Escolher a cor das conexões de localização"

# Tooltip: play area connection color
msgid "OP_playAreaConnectionColor_tooltip"
msgstr "Esta cor será usada para desenhar linhas\npara conexões de localização."

# Option: change custom playarea image on setup
msgid "OP_changePlayAreaImage"
msgstr "Mudar plano de fundo na configuração"

# Tooltip: change custom playarea image on setup
msgid "OP_changePlayAreaImage_tooltip"
msgstr "Tenta definir a área de jogo para uma imagem adequada\nda galeria de imagens da área de jogo."

# Group: playermat settings
msgid "OP_group_playermat"
msgstr "CONFIGURAÇÕES GLOBAIS DO TABULEIRO DO JOGADOR"

# Option: enable snap tags
msgid "OP_useSnapTags"
msgstr "Ativar tags de encaixe"

# Tooltip: enable snap tags
msgid "OP_useSnapTags_tooltip"
msgstr "Apenas cartas com a tag 'Vantagem' se encaixarão (cartas oficiais são suportadas por padrão).\nDesative isso se estiver tendo problemas com conteúdo personalizado."

# Option: show draw 1 button
msgid "OP_showDrawButton"
msgstr "Mostrar botão \"Comprar 1\""

# Tooltip: show draw 1 button
msgid "OP_showDrawButton_tooltip"
msgstr "Exibe um botão abaixo do botão 'Manutenção' que compra uma carta do seu deck.\nÚtil para jogo solo com várias mãos."

# Option: use class-specific texture
msgid "OP_useClassTexture"
msgstr "Usar textura específica da classe"

# Tooltip: use class-specific texture
msgid "OP_useClassTexture_tooltip"
msgstr "Controla se uma textura de tabuleiro do jogador específica da classe\ndeve ser carregada automaticamente."

# Option: use clickable clue-counters
msgid "OP_useClueClickers"
msgstr "Usar contadores de pistas clicáveis"

# Tooltip: use clickable clue-counters
msgid "OP_useClueClickers_tooltip"
msgstr "Em vez de contar automaticamente as pistas na área respectiva do seu tabuleiro, isso exibe um contador de pistas clicável."

# Option: use clickable resource counters
msgid "OP_useResourceCounters"
msgstr "Usar tokens de recurso clicáveis"

# Tooltip: use clickable resource counters
msgid "OP_useResourceCounters_tooltip"
msgstr "Isso ativa a criação de tokens de recurso clicáveis para cartas de jogador.\n(Escolha do Chef = vantagens com 0 usos)"

