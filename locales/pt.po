# Option: show hand helper
msgid "OP_showHandHelper"
msgstr "Ajudante de Mão"

# Tooltip: show hand helper
msgid "OP_showHandHelper_tooltip"
msgstr "Nunca mais conte suas cartas na mão! Esta ferramenta faz isso por você\ne até calcula seu tamanho máximo de mão."

# Option: show search assistant
msgid "OP_showSearchAssistant"
msgstr "Assistente de Busca"

# Tooltip: show search assistant
msgid "OP_showSearchAssistant_tooltip"
msgstr "Procure rapidamente as 3, 6, 9 ou as X cartas de cima\ndo seu deck!"

